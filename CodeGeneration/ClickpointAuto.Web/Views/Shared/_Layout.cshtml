<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <header>
         <div id="title">
         </div>
         <nav>
           <ul id="menu">
                    <li>@Html.ActionLink("Car Inventory", "Index", "Car")</li>
                    <li>@Html.ActionLink("Create New Car", "Create", "Car")</li>
                </ul>
         </nav>
        </header>
        <section id="main">
           <img src="@Url.Content("~/Content/Images/clickpoint_logo_highres.png")" alt="ClickPoint Logo"  height="100px" width="300px"/>
           <h1 id="title">ClickPoint Motors</h1>
            @RenderBody()
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>
